{
  "name": "aframe-site-dev",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "hexo": "^3.1.1",
    "hexo-admin": "^1.1.1",
    "hexo-deployer-git": "0.0.4",
    "hexo-generator-archive": "^0.1.3",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-tag": "^0.1.2",
    "hexo-renderer-ejs": "^0.1.0",
    "hexo-renderer-marked": "^0.2.5",
    "hexo-renderer-stylus": "^0.3.0",
    "hexo-server": "^0.1.2"
  },
  "devDependencies": {
    "hexo-cli": "^0.1.9"
  },
  "hexo": {
    "version": "3.1.1"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run server",
    "server": "hexo server",
    "generate": "hexo generate",
    "predeploy": "hexo clean",
    "deploy": "hexo deploy && open 'https://aframevr.github.io/'",
    "release": "npm run syncjs ; npm run deploy",
    "clean": "hexo clean",
    "update": "node update.js",
    "syncjs": "pushd ../aframe/ && npm run dist && popd && cp ../aframe/dist/* themes/aframe/source/releases/latest/. && cp ../aframe/dist/* themes/aframe/source/releases/0.1.0/. && git add . && git commit -m 'bump releases'",
    "postsyncjs": "pushd ../aframe/ && git checkout dist/ && popd"
  }
}
