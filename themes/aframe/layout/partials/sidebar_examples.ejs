<aside class="sidebar">

  <%- partial('sidebar_header') %>

  <div class="list">
    <h2 class="hidden-visually">Examples</h2>
    <% Object.keys(site.data.examples).forEach(function (sectionSlug) { %>
      <% var section = site.data.examples[sectionSlug] %>
      <h3 class="section-title"><%- section.title %></h3>
      <ul class="subnav examples-subnav">
        <% if (section.examples) { %>
          <% section.examples.forEach(function (example) { %>
            <li class="subnav-item"><a href="<%- url_for(example.url) %>" class="subnav-link<%- page.section === example.section && page.slug === example.slug ? ' current' : '' %>"><span class="sidebar__link__text"><%- example.title %></span></a></li>
          <% }) %>
        <% } %>
      </ul>
    <% }) %>
  </div>
</aside>
